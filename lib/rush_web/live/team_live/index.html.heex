<h1>Football Stats</h1>

<table>

  <thead>
    <tr>
      <th>Team</th>
      <th>Total rushing yards</th>
      <th></th>
    </tr>
  </thead>
  <tbody id="records">
    <%= for {key, %{"total_rushing_yards" => total_rushing_yards}} <- @teams do %>
      <tr id={key}>
        <td><%= key %></td>
        <td><%= total_rushing_yards %></td>
      </tr>
    <% end %>
  </tbody>
</table>

